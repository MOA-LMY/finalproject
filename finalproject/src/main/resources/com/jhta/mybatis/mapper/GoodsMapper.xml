<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  
  <mapper  namespace="com.jhta.mybatis.mapper.GoodsMapper" >
  	
  	<insert id="insert" parameterType="goods">
  	
  	insert into goods values(#{g_num},#{g_name},#{g_price},#{g_content},#{g_orgimg},
  	#{g_saveimg},sysdate,#{g_hit},#{g_ea},#{g_info},#{gc_num})
  	
  	</insert>
  	
  	
  <select id="list" resultType="goods">
 
 	select * from goods 
 
 </select>
 
 <select id="mainlist"  resultType="goods">
 	select *from(
		select *from goods order by g_regdate desc) where rownum = 1
 </select>

  	
  
  </mapper>